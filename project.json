{
  "title": "FileSize",
  "version": "0.0.1",
  "dependencies": { },
  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "System.Runtime": "4.0.20-*",
        "System.Console": "4.0.0-*"
      }
    }
  },
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "optimize": false,
        "warningsAsErrors": true,
        "allowUnsafe": false
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE" ],
        "optimize": true,
        "warningsAsErrors": true,
        "allowUnsafe": false
      }
    }
  }
}
